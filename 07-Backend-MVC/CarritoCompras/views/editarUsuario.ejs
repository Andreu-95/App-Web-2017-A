<br>
<h3>Editar Usuario</h3>

<form action="/Usuario/editarUsuario" method="post">
  <div class="form-group row">
    <label for="nombre" class="col-2 col-form-label">Nombre</label>
    <div class="col-10">
      <input class="form-control" type="text" placeholder="Ingrese su Nombre" id="nombre" name="nombre" value="<%= editado.nombre %>">
    </div>
  </div>
  <div class="form-group row">
    <label for="apellido" class="col-2 col-form-label">Apellido</label>
    <div class="col-10">
      <input class="form-control" type="text" placeholder="Ingrese su Apellido" id="apellido" name="apellido" value="<%= editado.apellido %>">
    </div>
  </div>
  <div class="form-group row">
    <label for="correo" class="col-2 col-form-label">Correo</label>
    <div class="col-10">
      <input class="form-control" type="email" placeholder="Ingrese su Correo" id="correo" name="correo" value="<%= editado.correo %>">
    </div>
  </div>
  <div class="form-group row">
    <label for="password" class="col-2 col-form-label">Password</label>
    <div class="col-10">
      <input class="form-control" type="password" placeholder="Ingrese su Password" id="password" name="password" value="<%= editado.password %>">
    </div>
  </div>
  <div class="form-group row">
    <label for="fechaNacimiento" class="col-2 col-form-label">Fecha Nacimiento</label>
    <div class="col-10">
      <input class="form-control" type="date" placeholder="Ingrese su Fecha de Nacimiento" id="fechaNacimiento" name="fechaNacimiento" value="<%= editado.fechaNacimiento %>">
    </div>
  </div>
  <input type="hidden" name="id" value="<%= editado.id %>">
  <button type="button" class="btn btn-outline-secondary">Bot√≥n</button>
  <button type="reset" class="btn btn-outline-success">Reset</button>
  <button type="submit" class="btn btn-outline-danger">Editar</button>
</form>
