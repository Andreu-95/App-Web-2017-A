<h3>Crear Usuario</h3>

<div class="form-group row">
  <label for="nombre" class="col-2 col-form-label">Text</label>
  <div class="col-10">
    <input class="form-control" type="text" placeholder="Ingrese su Nombre" id="nombre">
  </div>
</div>
<div class="form-group row">
  <label for="apellido" class="col-2 col-form-label">Text</label>
  <div class="col-10">
    <input class="form-control" type="text" placeholder="Ingrese su Apellido" id="apellido">
  </div>
</div>
<div class="form-group row">
  <label for="correo" class="col-2 col-form-label">Email</label>
  <div class="col-10">
    <input class="form-control" type="email" placeholder="Ingrese su Correo" id="correo">
  </div>
</div>
<div class="form-group row">
  <label for="password" class="col-2 col-form-label">Password</label>
  <div class="col-10">
    <input class="form-control" type="password" placeholder="Ingrese su Password" id="password">
  </div>
</div>
<div class="form-group row">
  <label for="fechaNacimiento" class="col-2 col-form-label">Date</label>
  <div class="col-10">
    <input class="form-control" type="date" placeholder="Ingrese su Fecha de Nacimiento" id="fechaNacimiento">
  </div>
</div>
